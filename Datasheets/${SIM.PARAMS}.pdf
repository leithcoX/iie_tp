     </mrow>
    </mrow>
   </msub><mo>=</mo>
   <msub>
    <mrow><mi>C</mi>
    </mrow>
    <mrow>
     <mrow><mi>d</mi><mi>e</mi><mi>p</mi>
      <msub>
       <mrow><mi>l</mi>
       </mrow>
       <mrow>
