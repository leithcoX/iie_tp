      <mrow><mi>I</mi>
      </mrow>
      <mrow>
       <mrow><mi>b</mi><mi>d</mi><mi>w</mi><mi>n</mi>
       </mrow>
      </mrow>
     </msub><mo>=</mo><mo>-</mo><mi>I</mi>
     <msub>
      <mrow><mi>S</mi>
      </mrow>
