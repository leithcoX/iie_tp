  <mfrac bevelled='true'>
   <mrow><mn>1</mn>
   </mrow>
   <mrow>
    <mrow><mi>T</mi><mi>S</mi><mi>T</mi><mi>O</mi><mi>P</mi>
    </mrow>
   </mrow>
  </mfrac>
 </mrow></math></div>
