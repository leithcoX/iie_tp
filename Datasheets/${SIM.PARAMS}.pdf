        </mrow>
       </mtd>
       <mtd>
        <mrow>
         <mstyle mathvariant='normal'>
          <mrow><mi>i</mi><mi>f</mi>
          </mrow>
         </mstyle><mspace width="10px" />
         <msub>
          <mrow><mi>V</mi>
          </mrow>
          <mrow><mi>D</mi>
          </mrow>
